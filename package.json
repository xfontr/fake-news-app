{
  "name": "cleverpy-test",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "dependencies": {
    "@fontsource/manrope": "^4.5.12",
    "@reduxjs/toolkit": "^1.8.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.65",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "axios": "^0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "sass": "npx sass src/sass/index.scss public/css/index.css",
    "sass:dev": "npx sass src/sass/index.scss public/css/index.css --watch",
    "build": "react-scripts build && sass",
    "test": "react-scripts test --watchAll=false",
    "test:dev": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "msw": "^0.47.4",
    "sass": "^1.55.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "src/**/*.ts",
      "!src/index.tsx",
      "!src/setupTests.ts"
    ]
  }
}
